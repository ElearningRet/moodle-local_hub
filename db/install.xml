<?xml version="1.0" encoding="UTF-8" ?>
<XMLDB PATH="local/moodleorg/db" VERSION="20090219" COMMENT="XMLDB file for moodle.org tables"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="../../../lib/xmldb/xmldb.xsd"
>
  <TABLES>
    <TABLE NAME="countries" COMMENT="countries table retrofitted from MySQL" NEXT="register_ads">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" SEQUENCE="true" ENUM="false" NEXT="ipfrom"/>
        <FIELD NAME="ipfrom" TYPE="int" LENGTH="20" NOTNULL="true" UNSIGNED="true" DEFAULT="0" SEQUENCE="false" ENUM="false" PREVIOUS="id" NEXT="ipto"/>
        <FIELD NAME="ipto" TYPE="int" LENGTH="20" NOTNULL="true" UNSIGNED="true" DEFAULT="0" SEQUENCE="false" ENUM="false" PREVIOUS="ipfrom" NEXT="code2"/>
        <FIELD NAME="code2" TYPE="char" LENGTH="2" NOTNULL="false" SEQUENCE="false" ENUM="false" PREVIOUS="ipto" NEXT="code3"/>
        <FIELD NAME="code3" TYPE="char" LENGTH="3" NOTNULL="false" SEQUENCE="false" ENUM="false" PREVIOUS="code2" NEXT="countryname"/>
        <FIELD NAME="countryname" TYPE="char" LENGTH="255" NOTNULL="false" SEQUENCE="false" ENUM="false" PREVIOUS="code3"/>
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
      </KEYS>
      <INDEXES>
        <INDEX NAME="ipfrom" UNIQUE="false" FIELDS="ipfrom, ipto" NEXT="code2"/>
        <INDEX NAME="code2" UNIQUE="false" FIELDS="code2" PREVIOUS="ipfrom" NEXT="code3"/>
        <INDEX NAME="code3" UNIQUE="false" FIELDS="code3" PREVIOUS="code2"/>
      </INDEXES>
    </TABLE>
    <TABLE NAME="register_ads" COMMENT="register_ads table retrofitted from MySQL" PREVIOUS="countries" NEXT="register_donations">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" SEQUENCE="true" ENUM="false" NEXT="country"/>
        <FIELD NAME="country" TYPE="char" LENGTH="2" NOTNULL="false" SEQUENCE="false" ENUM="false" PREVIOUS="id" NEXT="partner"/>
        <FIELD NAME="partner" TYPE="char" LENGTH="255" NOTNULL="false" SEQUENCE="false" ENUM="false" PREVIOUS="country" NEXT="title"/>
        <FIELD NAME="title" TYPE="char" LENGTH="255" NOTNULL="false" SEQUENCE="false" ENUM="false" PREVIOUS="partner" NEXT="image"/>
        <FIELD NAME="image" TYPE="char" LENGTH="255" NOTNULL="false" SEQUENCE="false" ENUM="false" PREVIOUS="title"/>
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
      </KEYS>
      <INDEXES>
        <INDEX NAME="country" UNIQUE="false" FIELDS="country"/>
      </INDEXES>
    </TABLE>
    <TABLE NAME="register_donations" COMMENT="register_donations table retrofitted from MySQL" PREVIOUS="register_ads" NEXT="register_downloads">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" SEQUENCE="true" ENUM="false" NEXT="userid"/>
        <FIELD NAME="userid" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" DEFAULT="0" SEQUENCE="false" ENUM="false" PREVIOUS="id" NEXT="name"/>
        <FIELD NAME="name" TYPE="char" LENGTH="255" NOTNULL="false" SEQUENCE="false" ENUM="false" PREVIOUS="userid" NEXT="org"/>
        <FIELD NAME="org" TYPE="char" LENGTH="255" NOTNULL="false" SEQUENCE="false" ENUM="false" PREVIOUS="name" NEXT="url"/>
        <FIELD NAME="url" TYPE="char" LENGTH="255" NOTNULL="false" SEQUENCE="false" ENUM="false" PREVIOUS="org" NEXT="amount"/>
        <FIELD NAME="amount" TYPE="char" LENGTH="100" NOTNULL="false" SEQUENCE="false" ENUM="false" PREVIOUS="url" NEXT="timedonated"/>
        <FIELD NAME="timedonated" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" DEFAULT="1053918331" SEQUENCE="false" ENUM="false" PREVIOUS="amount"/>
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
      </KEYS>
    </TABLE>
    <TABLE NAME="register_downloads" COMMENT="register_downloads table retrofitted from MySQL" PREVIOUS="register_donations" NEXT="registry">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" SEQUENCE="true" ENUM="false" NEXT="url"/>
        <FIELD NAME="url" TYPE="char" LENGTH="255" NOTNULL="true" SEQUENCE="false" ENUM="false" PREVIOUS="id" NEXT="ip"/>
        <FIELD NAME="ip" TYPE="char" LENGTH="255" NOTNULL="true" SEQUENCE="false" ENUM="false" PREVIOUS="url" NEXT="timeaccess"/>
        <FIELD NAME="timeaccess" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" DEFAULT="0" SEQUENCE="false" ENUM="false" PREVIOUS="ip"/>
